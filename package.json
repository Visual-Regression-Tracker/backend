    "pixelmatch": "^5.2.1",
    "pngjs": "^5.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "swagger-ui-express": "^4.3.0",
    "uuid-apikey": "^1.5.3"
