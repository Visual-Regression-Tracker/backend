    "@nestjs/platform-express": "^8.4.1",
    "@nestjs/platform-socket.io": "^8.4.1",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/terminus": "^8.0.8",
    "@nestjs/websockets": "^8.4.1",
    "@prisma/client": "2.12.1",
    "ajv": "^8.10.0",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^3.6.0",
